<!DOCTYPE html>
<!-- Website template by freewebsitetemplates.com -->
<html>
<head>
	<meta charset="UTF-8">
	<title>Dive Sites - Bhaccasyoniztas Beach Resort Website Template</title>
	<link rel="stylesheet" href="css/style.css" type="text/css">
</head>
<body>
	<div id="background">
		<div id="page">
			{{> header }}
			<div id="contents">
				<div class="box">
					<div>
						<div class="body">
							<h1>Travel</h1>
							<ul id="sites">
			{{#each trips}}	
								<li>
									<a href="/travel"><img src="images/{{this.image}}" alt="Img"></a>
									<h2><a href="/travel">{{this.name}}</a></h2>
									{{this.description}}
								</li>
								<li>
									<a href="/rooms"><img src="images/{{this.image}}" alt="Img"></a>
									<h2><a href="/rooms">{{this.name}}</a></h2>
									{{this.description}}
									<span class="rate">{{this.rate}}</span>
								</li>
								<li>
									<h2><a href="/meals">{{this.title}}</a></h2>
									<div class="infos">
										<a href="/meals"><img src="images/{{this.image}}" alt="Img" height="169" width="780"><span class="cover"></span></a>
											<span>{{this.name}}</span> {{this.description}}
								</li>
			{{/each}}					
							</ul>
						</div>
					</div>
				</div>
			</div>
		</div>
		{{> footer }}
	</div>
</body>
</html>